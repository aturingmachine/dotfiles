{
    "editor.fontSize": 12,
    "editor.suggest.showInlineDetails": true,
    "window.titleBarStyle": "custom",
    "editor.minimap.showSlider": "always",
    "editor.scrollbar.vertical": "hidden",
    "workbench.colorTheme": "Tokyo Night",
    "files.autoSave": "onFocusChange",
    "eslint.alwaysShowStatus": true,
    "editor.formatOnSave": true,
    "eslint.format.enable": true,
    "eslint.debug": false,
    "[typescript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[javascript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[vue]": {
        "editor.defaultFormatter": "octref.vetur"
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.stylelint": true
    },
    "files.associations": {
        ".env*": "dotenv"
    },
    "typescript.preferences.quoteStyle": "single",
    "typescript.tsdk": "node_modules/typescript/lib",
    "vim.leader": "\\",
    "vim.cursorStylePerMode.insert": "line",
    "vim.whichwrap": "b,s,>,<",
    "vim.easymotion": true,
    "vim.debug.loggingLevelForConsole": "verbose",
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "<C-a>"
            ],
            "after": [
                "A"
            ]
        },
        {
            "before": [
                "<C-f>"
            ],
            "after": [
                "/"
            ]
        }
    ],
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "v",
                "v"
            ],
            "after": [
                "<Esc>"
            ]
        },
        {
            "before": [
                "<C-]>"
            ],
            "after": [
                "<Esc>",
                "/",
                "\\",
                "s",
                "[",
                "(",
                "{",
                "\\",
                "[",
                "]",
                "<CR>",
                "i",
                "<Right>"
            ]
        },
        {
            "before": [
                "<C-[>"
            ],
            "after": [
                "<Esc>",
                "?",
                "\\",
                "s",
                "[",
                "(",
                "{",
                "\\",
                "[",
                "]",
                "<CR>",
                "i",
            ]
        }
    ],
    "vim.handleKeys": {
        "<C-k>": false,
        "<C-f>": false,
        "<C-v>": false,
        "<C-c>": false,
        "<C-x>": false,
        "<C-a>": false,
        "<C-w>": false
    },
    "editor.tabSize": 2,
    "editor.fontFamily": "\"Fira Code\"",
    "editor.fontLigatures": true,
    "editor.inlineHints.fontFamily": "'Fira Code'",
    "editor.fontWeight": "500",
    "diffEditor.wordWrap": "on",
    "editor.wordWrap": "on",
    "window.menuBarVisibility": "toggle",
    "files.insertFinalNewline": true,
    "gitlens.codeLens.enabled": false,
    "gitlens.defaultDateShortFormat": "YYMMDD",
    "gitlens.defaultDateFormat": "YYMMDD H:mm",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "keyboard.dispatch": "keyCode",
    "workbench.productIconTheme": "fluent-icons",
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "* @todo"
    ],
    "stylelint.validate": [
        "css",
        "less",
        "postcss",
        "scss",
        "vue"
    ],
    "javascript.updateImportsOnFileMove.enabled": "always",
    "tsimporter.emitSemicolon": false,
    "eslint.workingDirectories": [
        {
            "directory": "./liger-hub-client",
            "changeProcessCWD": true
        }
    ],
    "editor.accessibilitySupport": "off",
    "workbench.iconTheme": "material-icon-theme",
    "material-icon-theme.folders.theme": "specific"
}
